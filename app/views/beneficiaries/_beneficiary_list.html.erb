<table>
  <thead>
    <tr>
      <th><%= t '.number' %></th>
      <th><%= t '.name' %></th>
      <th colspan="6"></th>
    </tr>
  </thead>
  <tbody>
    <% beneficiary_list.each do |beneficiary| %>
      <% if beneficiary.user == current_user %>
        <tr>
          <td><%= beneficiary.account_number %></td>
          <td><%= beneficiary.nick_name %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
